<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CatalogCategoryTextAppearance" parent="">
        <item name="fontFamily">sans-serif</item>
        <item name="android:textSize">16sp</item>
    </style>
    <style name="ButtonTextAppearance" parent="">
        <item name="fontFamily">sans-serif-medium</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="ToolBarTitleTextAppearance" parent="">
        <item name="fontFamily">sans-serif-medium</item>
        <item name="android:textSize">22sp</item>
    </style>

    <style name="ConversationItemView.Title" parent="TextAppearance.AppCompat">
        <item name="fontFamily">sans-serif-medium</item>
