<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@+id/checked"
        android:state_checked="true"
        android:drawable="@drawable/ic_like_24_filled" />
    <item
        android:id="@+id/unchecked"
        android:drawable="@drawable/ic_like_24_outline" />
    <transition
        android:fromId="@+id/unchecked"
        android:toId="@+id/checked"
        android:drawable="@drawable/ic_like_outline_to_filled_animator" />
    <transition
        android:fromId="@+id/checked"
        android:toId="@+id/unchecked"
        android:drawable="@drawable/ic_like_filled_to_outline_animator" />
</animated-selector>
